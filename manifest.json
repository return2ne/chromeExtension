{
  "manifest_version": 2,
  "name": "chrome extension sample",
  "description": "chrome extension sample page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "geolocation",
    "cookies",
    "*://*.nexon.com/"
  ]
}
