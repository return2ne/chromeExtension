{
	"manifest_version": 2,
	"name": "chrome extension sample",
	"description": "chrome extension sample pabe",
	"version": "1.0",

	"offline_enabled": false,
	"browser_action": {
		"default_icon": "icon.png"
	},
	"background": { 
		"scripts": ["background.js"],
		"persistent": false
	},
	"chrome_url_overrides" : {
		"newtab": "newtab.html"
	},
	"permissions": ["tabs", "geolocation", "cookies", "*://*.nexon.com/"]
}
